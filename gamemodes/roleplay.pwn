	SendClientMessageEx(userid, COLOR_YELLOW, "(( PM from %s (%d): %s ))", ReturnName(playerid, 0), playerid, text);
	SendClientMessageEx(playerid, COLOR_YELLOW, "(( PM to %s (%d): %s ))", ReturnName(userid, 0), userid, text);
